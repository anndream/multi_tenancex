<h2>Show company</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @company.name %>
  </li>

  <li>
    <strong>Slug:</strong>
    <%= @company.slug %>
  </li>

  <li>
    <strong>Image:</strong>
    <%= @company.image %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @company.description %>
  </li>

</ul>

<span><%= link "Edit", to: admin_company_path(@conn, :edit, @company) %></span>
<span><%= link "Back", to: admin_company_path(@conn, :index) %></span>
